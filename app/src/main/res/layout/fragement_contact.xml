<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/contact_root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/contact_recyclerview"
        android:background="@color/contact_background_color"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v7.widget.RecyclerView>

    <com.jb.goscanner.function.widget.WaveSideBarView
        android:id="@+id/contact_wavesidebarview"
        app:sidebarBackgroundColor="#4072ff"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>
